    <script src="~/js/jquery-2.2.3.min.js"></script>
    <script>
        var minutes = 0;
        var seconds = 0;
        var minutes1 = 0;
        var seconds1 = 0;
        var minutes2 = 0;
        var seconds2 = 0;

        $(document).ready(function () {
            debugger;
            var timer2 = $("#hdfDuration").val();
            var interval = setInterval(function () {
                var timer = timer2.split(':');
                minutes = parseInt(timer[0], 10);
                seconds = parseInt(timer[1], 10);
                --seconds;
                minutes = (seconds < 0) ? --minutes : minutes;
                if (minutes < 0) {
                    clearInterval(interval);
                }
                seconds = (seconds < 0) ? 59 : seconds;
                seconds = (seconds < 10) ? '0' + seconds : seconds;
                if (minutes == 0 && seconds == 0) {
                    clearInterval(interval);
                    window.location.href = "/User/UserDashboard";
                }
                if (minutes == 0 && seconds == 30) {
                    getResponse();
                }
                $('.digit').html(minutes + ':' + seconds);
                timer2 = minutes + ':' + seconds;
            }, 1000);
            //function gfg_Run() {
            //    setInterval(function () {

            //    }, 180000);
            //}
        });
        function getResponse() {
            $.ajax({
                url: "/User/GetGameResponse",
                type: "Get",
                datatype: "Json",
                data: { PeriodId: $("#hdfPeriodId").val(), ColorId: $("#FK_ColorId").val() },
                success: function (response) {
                    debugger;
                    if (response != null) {
                        if (response.Message == "1") {
                            if (response.Result == "Yes") {
                                //alert("Congrats! You won.");
                            }
                            else {
                                //alert("Oops! You lose.");
                            }
                            if (response.FK_ColorId == "1") {
                                $("#spColor").text("R");
                                $(".body-color").attr("style", "background-color: red;");
                            }
                            else if (response.FK_ColorId == "2") {
                                $("#spColor").text("G");
                                $(".body-color").attr("style", "background-color: green;");
                            }
                            else if (response.FK_ColorId == "3") {
                                $("#spColor").text("B");
                                $(".body-color").attr("style", "background-color: blue;");
                            }
                            else {
                            }
                        }
                        else {
                            //alert(response.Result);
                        }
                    }
                }
            });
        }
        $(".dash-nav").click(function () {
            $(".digit").show();
            $(".first-view").hide();
            $(".game-view").show();
        });
        var ColorId = "";
        function GameDetailsById(GameId) {
            debugger;
            $("#hdfGameId").val(GameId);
            $.ajax({
                url: "/User/GameDetailsById",
                type: "Get",
                datatype: "Json",
                data: { GameId: GameId },
                success: function (response) {
                    debugger;
                    if (response != null) {
                        $("#spPeriodNo").text(response.PeriodNo);
                        $("#hdfStartTime").val(response.StartTime);
                        $("#hdfEndTime").val(response.EndTime);
                        $("#hdfPeriodId").val(response.FK_PeriodId);
                        $("#hdfDuration").val(response.duration);
                        if (response.lstPrediction != null) {
                            $("#table1 tbody tr td").remove();
                            $("#table2 tbody tr td").remove();
                            $("#table3 tbody tr td").remove();
                            $("#table4 tbody tr td").remove();
                            for (var i = 0; i < response.lstPrediction.length; i++) {
                                if (GameId == "7") {
                                    $("#table1").append('<tbody><tr><td>' + response.lstPrediction[i].PeriodNo + '</td><td class="text-' + response.lstPrediction[i].ColorName + '">' + response.lstPrediction[i].ColorName + '</td><td class="text-center">' + response.lstPrediction[i].Amount + '</td></tr></tbody></table>');
                                    $("#table2").append('<tbody><tr><td>' + response.lstPrediction[i].PeriodNo + '</td><td class="text-' + response.lstPrediction[i].ColorName + '">' + response.lstPrediction[i].ColorName + '</td><td class="text-center"><button type="button" class="btn btn-' + response.lstPrediction[i].ResultantColor + ' btn-lg">' + response.lstPrediction[i].ResultantColor + '</button></td></tr></tbody>');
                                }
                                else if (GameId == "8") {
                                    $("#table3").append('<tbody><tr><td>' + response.lstPrediction[i].PeriodNo + '</td><td class="text-' + response.lstPrediction[i].ColorName + '">' + response.lstPrediction[i].ColorName + '</td><td class="text-center">' + response.lstPrediction[i].Amount + '</td></tr></tbody></table>');
                                    $("#table4").append('<tbody><tr><td>' + response.lstPrediction[i].PeriodNo + '</td><td class="text-' + response.lstPrediction[i].ColorName + '">' + response.lstPrediction[i].ColorName + '</td><td class="text-center"><button type="button" class="btn btn-' + response.lstPrediction[i].ResultantColor + ' btn-lg">' + response.lstPrediction[i].ResultantColor + '</button></td></tr></tbody>');
                                }
                            }
                        }
                        if (GameId == "8") {
                            minutes = 0;
                            seconds = 0;
                            minutes1 = 0;
                            seconds1 = 0;
                            minutes2 = 0;
                            seconds2 = 0;
                            $('.digit').html('');
                            $('.digit1').html('');
                            $('.digit2').html('');
                            $('.time-counter').html('<span class="digit1"></span>');
                            var timer3 = $("#hdfDuration").val();
                            var interval1 = setInterval(function () {
                                var timer1 = timer3.split(':');
                                minutes1 = parseInt(timer1[0], 10);
                                seconds1 = parseInt(timer1[1], 10);
                                --seconds1;
                                minutes1 = (seconds1 < 0) ? --minutes1 : minutes1;
                                if (minutes1 < 0) {
                                    clearInterval(interval1);
                                }
                                seconds1 = (seconds1 < 0) ? 59 : seconds1;
                                seconds1 = (seconds1 < 10) ? '0' + seconds1 : seconds1;
                                if (minutes1 == 0 && seconds1 == 0) {
                                    clearInterval(interval1);
                                    window.location.href = "/User/UserDashboard";
                                }
                                if (minutes1 == 0 && seconds1 == 30) {
                                    getResponse();
                                }
                                $('.digit1').html(minutes1 + ':' + seconds1);
                                timer3 = minutes1 + ':' + seconds1;
                            }, 1000);
                        }
                        else if (GameId == "7") {
                            minutes = 0;
                            seconds = 0;
                            minutes1 = 0;
                            seconds1 = 0;
                            minutes2 = 0;
                            seconds2 = 0;
                            $('.digit1').html('');
                            $('.digit').html('');
                            $('.digit2').html('');
                            $('.time-counter').html('<span class="digit2"></span>');
                            var timer4 = $("#hdfDuration").val();
                            var interval2 = setInterval(function () {
                                var timer3 = timer4.split(':');
                                minutes2 = parseInt(timer3[0], 10);
                                seconds2 = parseInt(timer3[1], 10);
                                --seconds2;
                                minutes2 = (seconds2 < 0) ? --minutes2 : minutes2;
                                if (minutes2 < 0) {
                                    clearInterval(interval2);
                                }
                                seconds2 = (seconds2 < 0) ? 59 : seconds2;
                                seconds2 = (seconds2 < 10) ? '0' + seconds2 : seconds2;
                                if (minutes2 == 0 && seconds2 == 0) {
                                    clearInterval(interval2);
                                    window.location.href = "/User/UserDashboard";
                                }
                                if (minutes2 == 0 && seconds2 == 30) {
                                    getResponse();
                                }
                                $('.digit2').html(minutes2 + ':' + seconds2);
                                timer4 = minutes2 + ':' + seconds2;
                            }, 1000);
                        }
                        else {

                        }
                    }
                }
            });
        }
        function SetPrediction(ColorId) {
            debugger;
            if ($("#hdfGameId").val() == "7" && minutes == 0 && seconds > 0 && seconds < 30) {
                $('#myModal').modal('hide');
                alert("Your game time is over.")
            }
            else if ($("#hdfGameId").val() == "7" && minutes2 == 0 && seconds2 > 0 && seconds2 < 30) {
                $('#myModal').modal('hide');
                alert("Your game time is over.")
            }
            else if (($("#hdfGameId").val() == "8") && (minutes1 == 0 && seconds1 > 0 && seconds1 < 30)) {
                $('#myModal').modal('hide');
                alert("Your game time is over.")
            }
            else {
                $('#myModal').modal('show');
                if ($("#hdfGameId").val() == "7") {
                    if (ColorId == 1) {
                        $("#btnColor").attr("style", "background-color:red");
                        $("#btnColor").text("R");
                        $("#FK_ColorId").val(ColorId);
                    }
                    else if (ColorId == 2) {
                        $("#btnColor").attr("style", "background-color:green");
                        $("#btnColor").text("G");
                        $("#FK_ColorId").val(ColorId);
                    }
                    else if (ColorId == 3) {
                        $("#btnColor").attr("style", "background-color:blue");
                        $("#btnColor").text("B");
                        $("#FK_ColorId").val(ColorId);
                    }
                    else {
                    }
                }
                else if ($("#hdfGameId").val() == "8") {
                    $("#btnColor").text(ColorId);
                    $("#FK_ColorId").val(ColorId);
                }
                else {
                }
            }
        }

